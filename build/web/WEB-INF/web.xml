<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>user_reg</servlet-name>
        <servlet-class>Servelets.user_reg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>Servelets.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>Servelets.logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SendResetPasswordEmail</servlet-name>
        <servlet-class>Servelets.SendResetPasswordEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResetPw</servlet-name>
        <servlet-class>Servelets.ResetPw</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadProducts</servlet-name>
        <servlet-class>Servelets.LoadProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToTable</servlet-name>
        <servlet-class>Servelets.AddToTable</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegProducts</servlet-name>
        <servlet-class>Servelets.RegProducts</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addtocart</servlet-name>
        <servlet-class>Servelets.addtocart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RemoveFromCart</servlet-name>
        <servlet-class>Servelets.RemoveFromCart</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>purchase</servlet-name>
        <servlet-class>Servelets.purchase</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>user_reg</servlet-name>
        <url-pattern>/user_reg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SendResetPasswordEmail</servlet-name>
        <url-pattern>/SendResetPasswordEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResetPw</servlet-name>
        <url-pattern>/ResetPw</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadProducts</servlet-name>
        <url-pattern>/LoadProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToTable</servlet-name>
        <url-pattern>/AddToTable</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegProducts</servlet-name>
        <url-pattern>/RegProducts</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addtocart</servlet-name>
        <url-pattern>/addtocart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RemoveFromCart</servlet-name>
        <url-pattern>/RemoveFromCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>purchase</servlet-name>
        <url-pattern>/purchase</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
